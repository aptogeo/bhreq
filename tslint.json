{
  "extends": ["tslint:recommended"],
  "rules": {
    "trailing-comma": [false, {
      "multiline": "always",
      "singleline": "never"
    }],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [false],
    "max-line-length": [false],
    "arrow-parens": false,
    "max-classes-per-file": [false],
    "object-literal-key-quotes": [false],
    "no-console": [true, "log"]
  }
}
